# Deploy configuration for scripts/deploy.sh

# Registry credentials / destination
DOCKER_REGISTRY=ghcr.io
GITHUB_USERNAME=your-github-username
GITHUB_REPO=your-repo-name
GITHUB_TOKEN=ghp_yourTokenGoesHere

# Tag + optional local image prefix (mirrors build config)
IMAGE_TAG=latest
IMAGE_NAME_PREFIX=
IMAGE_PACKAGE_PREFIX=
DEPLOY_ALLOW_PUSH=false
